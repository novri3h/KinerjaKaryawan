Private Sub Simpan()
Dim Periksa As Boolean
Dim Pendidikan As String
Periksa = IIf(Text1.Text = "" Or Text2.Text = "" Or Combo1.Text = "" Or Text3.Text = "" Or Text4.Text = "", False, True)
If Periksa Then
    If Edit Then
        RecNilai.Seek "=", Text1.Text, Text4.Text
        RecNilai.Edit
    Else
        RecNilai.AddNew
    End If
    RecNilai!nip_peg = Trim(Text1.Text)
    RecNilai!nippen = Trim(Text4.Text)
    RecNilai!tglnilai = tgl.Value
    RecNilai!lmkenal = Val(Text5.Text)
